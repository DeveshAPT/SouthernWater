<?xml version="1.0" encoding="UTF-8"?>
<locators>
<EmailId>//input[@id='inpEmail']</EmailId>
<Password>//input[@id='customerPassword']</Password>
<LoginButton>//button[@type='submit']</LoginButton>
<Register>//a[contains(text(),'Register')]</Register>
<Forgot>//a[contains(text(),'Forgot your password?')]</Forgot>
<ForgotEmail>//input[@id='userEmail']</ForgotEmail>
<BackTologin>//a[contains(text(),'Back to login')]</BackTologin>
<ForgotContinue>//button[text()='Continue']</ForgotContinue>
<LoginError>//div[@class='feedback-box mb-4 feedback-box--harley']//p</LoginError>
<ForgotMessageEmail>//div[@class='sw-body-container']//p//strong</ForgotMessageEmail>
<ForgotMessage>//div[@class='sw-body-container']//p</ForgotMessage>
</locators>

